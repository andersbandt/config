#include <stdint.h>



int compareByteArrays(const uint8_t *arr1, const uint8_t *arr2, size_t len) {
    for (size_t i = 0; i < len; ++i) {
        if (arr1[i] != arr2[i]) {
            return 0; // Not equal
        }
    }
    return 1; // Equal
}
